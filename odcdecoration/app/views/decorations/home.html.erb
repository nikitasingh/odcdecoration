
<br/>
 <div class="menu"> Select year:<%= select_tag("years", options_for_select(Decoration.select("DISTINCT(YEAR)").collect{|u|[u.year]}),:prompt => "All",:onchange=>"showDecoration(this.value);") %>


</div>
<center>
<h1>Listing decorations</h1>

<div id="year_list"  style="width : 100%; height : 500px; overflow : auto; ">
<table>
  <tr>
    <th>Name</th>
    <th>Year</th>
    <th>Zone</th>
   
  </tr>

<% @decorations.each do |dec| %>
<% @zone=Zone.find(dec.zone_id) %>

  <tr>
    <td><%= dec.name %></td>
    <td><%= dec.year %></td>
    <td><%= @zone.name %></td>
    
  </tr>
<% end %>
</table>

<br />


</div>
