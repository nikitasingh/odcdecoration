 <div class="actions">

  </div>
  <h1>Listing decorations</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Year</th>
    <th>Zone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @decorations.each do |decoration| %>
  <tr>
    <td><%= decoration.name %></td>
    <td><%= decoration.year %></td>
    <td><%= decoration.zone_id %></td>
    <td><%= link_to 'Assign tasks', decoration %></td>
    <td><%= link_to 'Show', decoration %></td>
    <td><%= link_to 'Edit', edit_decoration_path(decoration) %></td>
    <td><%= link_to 'Destroy', decoration, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

